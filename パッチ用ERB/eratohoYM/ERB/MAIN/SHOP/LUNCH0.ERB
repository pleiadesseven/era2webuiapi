	LOCAL += 1

;昼の場合は夜までｷｬｯｷｬｳﾌﾌ
IF TIME == 0 && LOCAL > 3
	TIME += 1
	LOCAL += 2
ENDIF

;恐怖刻印3の場合は実行しにくくなる。
SIF MARK:恐怖刻印 > 2 && LOCAL
	LOCAL -= 1

;反発刻印があると実行しにくくなる。また、恋慕なし反発3では発生せず
IF MARK:反発刻印 && LOCAL
	IF MARK:反発刻印 >= 3 && TALENT:恋慕 == 0
		LOCAL = 0
	ELSE
		LOCAL /= 1 + MARK:反発刻印
		LOCAL -= MARK:反発刻印
	ENDIF
ENDIF
IF LOCAL < 1
	TFLAG:202 = 0
	RETURN 0
ENDIF

DRAWLINE
PRINTFORML 料理を食べた%CALLNAME:MASTER%と%CALLNAME:TARGET%はなんとも言えない感覚に襲われた
PRINTL 
PRINTL どうする？
PRINTL [0] - 押し倒したい欲望に耐える
PRINTFORML [1] - %CALLNAME:TARGET%を押し倒す
DRAWLINE
$SELECT_POINT_EXERCISE
INPUT
IF RESULT == 0
	PRINTFORML %CALLNAME:MASTER%は渦巻く欲望を抑え込み、%CALLNAME:TARGET%を残して自分の部屋へと向かった……
	PRINTL 
	WAIT
	;欲望LV＋自慰中毒LVが基本回数
	LOCAL = ABL:欲望 + ABL:自慰中毒
	;淫乱
	SIF TALENT:淫乱
		LOCAL += 1
	;發情
	SIF TALENT:常時発情 || (TALENT:危険日 && TALENT:発情可)
		LOCAL += 1
	;自制心
	SIF TALENT:自制心
		LOCAL -= 2
	;快楽の否定
	SIF TALENT:快感の否定
		LOCAL -= 2
	;反発刻印
	SIF MARK:反発刻印
		LOCAL -= MARK:反発刻印
	LOCAL /= 5
	LOCAL -= 3
	SIF TFLAG:202 == 2
		LOCAL += 5

	;アナニーパッチが有効でなおかつＡ調教フィルタがオフならならアナニーの可能性あり、ただし通常よりアナニーの可能性を減らしている
	IF FLAG:15 & 4 && (FLAG:2 & 2) == 0
		;自慰、アナニー判定。ポイントが同じ場合は自慰優先(Ｃ感覚が０の場合はアナニーのみ)
		;Ａ感覚見る
		LOCAL:1 = ABL:Ａ感覚
		;Ｃ感覚見る
		LOCAL:2 = 1 + ABL:Ｃ感覚

		;尻穴狂いだとアナニーしやすい+
		;淫核／淫茎だと通常の自慰しやすい+
		IF TALENT:淫尻
			LOCAL:1 += 1
		ELSEIF TALENT:淫核／淫茎
			LOCAL:2 += 1
		ENDIF
	ELSE
		;アナニーパッチが有効でないなら両方の判定を0に(強制的に通常自慰になる)
		LOCAL:1 = 0
		LOCAL:2 = 0
	ENDIF

	IF LOCAL > 0
		TFLAG:201 = LOCAL
		;貞操帯が自慰を妨害する(自慰、アナニーの違いはこの場合は表示だけ)
		IF CFLAG:42 & 2
			;素敵な食後の自慰を華麗に防御する貞操帯
			CALL DISPLAY_KOJO_MESSAGE_SUB_EVENT, TARGET, 442, (LOCAL:1 < LOCAL:2) ? 1 # 0
			PRINTL 
			;Ｖ経験
			SIF TALENT:オトコ == 0
				CALL COMMON_UP_EXP, TARGET, 0, LOCAL, 0
			;Ａ経験
			CALL COMMON_UP_EXP, TARGET, 1, LOCAL, 0
			;尿道開発経験
			CALL COMMON_UP_EXP, TARGET, 46, LOCAL, 0
		;通常のオナニー
		ELSEIF LOCAL:1 < LOCAL:2
			;素敵な食後の自慰(TFLAG:99 == 1 でオナニー)
			CALL DISPLAY_KOJO_MESSAGE_SUB_EVENT, TARGET, 441, 1
			PRINTL 
			;自慰経験
			CALL COMMON_UP_EXP, TARGET, 10, LOCAL, 0
		;食後にアナニー
		ELSE
			;素敵な食後のアナニー(TFLAG:99 == 2でアナニー,LOCAL:1<LOCAL:2の比較はたぶんできなくなると思う)
			CALL DISPLAY_KOJO_MESSAGE_SUB_EVENT, TARGET, 441, 2
			PRINTL 
			;Ａ経験
			CALL COMMON_UP_EXP, TARGET, 1, LOCAL, 0
			;自慰経験
			CALL COMMON_UP_EXP, TARGET, 10, LOCAL, 0
		ENDIF
	ENDIF
ELSEIF RESULT == 1
	SIF TFLAG:202 == 2
		LOCAL += 2
	TFLAG:201 = LOCAL
	;タイプわけ（棒ありＶありが1,棒ありＶなしが2,棒なしが3）
	IF TALENT:MASTER:オトコ || TALENT:MASTER:ふたなり
		IF TALENT:オトコ == 0
			;素敵な食後の運動Ｖ性交編
			CALL DISPLAY_KOJO_MESSAGE_SUB_EVENT, TARGET, 440, 1
			PRINTL 
			;Ｖ経験
			CALL COMMON_UP_EXP, TARGET, 0, LOCAL, 0
			;Ｖ調律経験
			CALL COMMON_UP_EXP, TARGET, 42, LOCAL, 0
			;処女食い回数カウント(恋慕持ちのみ)
			SIF TALENT:処女 && TALENT:恋慕
				FLAG:400 += 1
			;奉仕快楽経験(相愛・服従等を持っていた場合)
			SIF TFLAG:202 == 2
				CALL COMMON_UP_EXP, TARGET, 20, 2, 0
			;[処女]喪失
			TALENT:処女 = 0
		ELSE
			;素敵な食後の運動アナル性交編
			CALL DISPLAY_KOJO_MESSAGE_SUB_EVENT, TARGET, 440, 2
			PRINTL 
			;Ａ経験
			CALL COMMON_UP_EXP, TARGET, 1, LOCAL, 0
			;ＢＬ経験
			CALL COMMON_UP_EXP, TARGET, 13, LOCAL, 0
			;Ａ快楽経験(相愛・服従等を持っていた場合)
			SIF TFLAG:202 == 2
				CALL COMMON_UP_EXP, TARGET, 43, 2, 0
		ENDIF
	ELSE
		;素敵な食後の運動レズ編
		CALL DISPLAY_KOJO_MESSAGE_SUB_EVENT, TARGET, 440, 3
		PRINTL 
		;レズ経験
		CALL COMMON_UP_EXP, TARGET, 12, LOCAL, 0
		;Ｃ調律経験
		CALL COMMON_UP_EXP, TARGET, 41, LOCAL, 0
	ENDIF
ELSE
	GOTO SELECT_POINT_EXERCISE
ENDIF
WAIT
TFLAG:202 = 0
