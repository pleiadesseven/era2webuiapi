	CASE IS < 100
		LOCAL:1 = 0
	CASE IS < 200
		LOCAL:1 = 10
	CASE IS < 500
		LOCAL:1 = 25
	CASE IS < 1000
		LOCAL:1 = 50
	CASE IS < 3000
		LOCAL:1 = 75
	CASE IS < 7500
		LOCAL:1 = 100
	CASE IS < 15000
		LOCAL:1 = 250
	CASE IS < 50000
		LOCAL:1 = 500
	CASE IS < 100000
		LOCAL:1 = 800
	CASE IS < 500000
		LOCAL:1 = 1200
	CASE IS < 1000000
		LOCAL:1 = 2400
	CASE IS < 10000000
		LOCAL:1 = 5000
	CASEELSE
		LOCAL:1 = 10000
ENDSELECT

