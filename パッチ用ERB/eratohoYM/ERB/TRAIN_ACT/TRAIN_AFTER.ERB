SIF TALENT:臆病
	TIMES LOCAL:1 , 1.50
;[自制心]
SIF TALENT:自制心
	TIMES LOCAL:1 , 0.75
;[悲観的]
SIF TALENT:悲観的
	TIMES LOCAL:1 , 0.75

;調教度
IF CFLAG:3668 <= -700
	TIMES LOCAL:1 , 1.30
ELSEIF CFLAG:3668 <= -350
	TIMES LOCAL:1 , 1.20
ELSEIF CFLAG:3668 <= 0
	TIMES TFLAG:550, 1.10
