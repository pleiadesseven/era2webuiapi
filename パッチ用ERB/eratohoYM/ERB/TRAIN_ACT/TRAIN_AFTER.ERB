SIF TALENT:死亡
	LOCAL:1 = 0

CFLAG:ストレス += LOCAL:1
SIF TALENT:MASTER:998 && LOCAL:1
	PRINTFORML ストレス＋{LOCAL:1}
;ストレス値は100万以上にはならない
CFLAG:ストレス = LIMIT(CFLAG:ストレス, 0, 999999)

